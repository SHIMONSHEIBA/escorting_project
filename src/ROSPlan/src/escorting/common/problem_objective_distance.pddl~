(define (problem testdomain_task)
(:domain testdomain)
(:objects
    person1 person2 person3 - person
    robot1 robot2 - robot
    wp_dock1 wp_dock2 wp1 wp2 wp3 wp4 wp5 - waypoint
)
(:init
    (allowed_goto_waypoint robot1)
    (allowed_goto_waypoint robot2)
    (at 0 (arrived person1))
    (at 8 (arrived person2))
    (at 70 (arrived person3))
    (destination_waypoint wp2)
    (destination_waypoint wp3)
    (destination_waypoint wp4)
    (dock_at wp_dock1)
    (dock_at wp_dock2)
    (docked robot1)
    (docked robot2)
    (free_person person1)
    (free_person person2)
    (free_person person3)
    (free_robot robot1)
    (free_robot robot2)
    (general_waypoint wp_dock1)
    (general_waypoint wp_dock2)
    (general_waypoint wp1)
    (general_waypoint wp2)
    (general_waypoint wp3)
    (general_waypoint wp4)
    (general_waypoint wp5)
    (greeting_waypoint wp1)
    (is_on robot1)
    (is_on robot2)
    (at 0 (person_at person1 wp1))
    (at 8 (person_at person2 wp1))
    (at 70 (person_at person3 wp1))
    (robot_at robot1 wp_dock1)
    (robot_at robot2 wp_dock2)
    (= (battery robot1) 100)
    (= (battery robot2) 100)
    (= (total_stay person1) 1)
    (= (total_stay person2) 1)
    (= (total_stay person3) 1)
    (= (charging-rate robot1) 1.05)
    (= (charging-rate robot2) 1.05)
    (= (consumption-rate robot1) 0.9)
    (= (consumption-rate robot2) 0.9)
    (= (distance_x_y wp_dock1 wp1) 8.00016)
    (= (distance_x_y wp_dock1 wp2) 14.1524)
    (= (distance_x_y wp_dock1 wp3) 10.8859)
    (= (distance_x_y wp_dock1 wp4) 19.0018)
    (= (distance_x_y wp_dock1 wp5) 0.5)
    (= (distance_x_y wp_dock2 wp1) 8.40836)
    (= (distance_x_y wp_dock2 wp2) 14.2348)
    (= (distance_x_y wp_dock2 wp3) 9.90019)
    (= (distance_x_y wp_dock2 wp4) 18.7397)
    (= (distance_x_y wp_dock2 wp5) 0.5)
    (= (distance_x_y wp1 wp2) 6.87373)
    (= (distance_x_y wp1 wp3) 13.0993)
    (= (distance_x_y wp1 wp4) 13.2082)
    (= (distance_x_y wp1 wp5) 0.3)
    (= (distance_x_y wp2 wp3) 0.9)
    (= (distance_x_y wp2 wp4) 0.7)
    (= (distance_x_y wp2 wp5) 0.5)
    (= (distance_x_y wp3 wp4) 0.5)
    (= (distance_x_y wp3 wp5) 0.5)
    (= (distance_x_y wp4 wp5) 0.5)
    (= (distance_x_y wp1 wp_dock1) 8.00016)
    (= (distance_x_y wp2 wp_dock1) 14.1524)
    (= (distance_x_y wp3 wp_dock1) 10.8859)
    (= (distance_x_y wp4 wp_dock1) 19.0018)
    (= (distance_x_y wp5 wp_dock1) 0.5)
    (= (distance_x_y wp1 wp_dock2) 8.40836)
    (= (distance_x_y wp2 wp_dock2) 14.2348)
    (= (distance_x_y wp3 wp_dock2) 9.90019)
    (= (distance_x_y wp4 wp_dock2) 18.7397)
    (= (distance_x_y wp5 wp_dock2) 0.5)
    (= (distance_x_y wp2 wp1) 6.87373)
    (= (distance_x_y wp3 wp1) 13.0993)
    (= (distance_x_y wp4 wp1) 13.2082)
    (= (distance_x_y wp5 wp1) 0.3)
    (= (distance_x_y wp3 wp2) 0.9)
    (= (distance_x_y wp4 wp2) 0.7)
    (= (distance_x_y wp5 wp2) 0.5)
    (= (distance_x_y wp4 wp3) 0.5)
    (= (distance_x_y wp5 wp3) 0.5)
    (= (distance_x_y wp5 wp4) 0.5)

    (= (staying-rate person1) 1)
    (= (staying-rate person2) 2)
    (= (staying-rate person3) 4)
    (= (velocity robot1) 7)
    (= (velocity robot2) 2)
)
(:goal (and
    (person_guided person1)
    (person_guided person2)
    (person_guided person3)
    (person_greeted person1)
    (person_greeted person2)
    (person_greeted person3)
    (person_at person1 wp2)
    (person_at person2 wp3)
    (person_at person3 wp4)
    (docked robot1)
    (docked robot2)
    (staying person1)
    (staying person2)
    (staying person3)
))

(:metric minimize (+ (+ (total_stay person1) (total_stay person2)) (total_stay person3))) 

)
